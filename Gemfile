source "https://rubygems.org"

# Use GitHub Pages gem instead of individual theme
gem "github-pages", group: :jekyll_plugins

group :jekyll_plugins do
    gem "jekyll-paginate"
    gem "jekyll-sitemap"
    gem "jekyll-gist"
    gem "jekyll-feed"
    gem "jekyll-seo-tag"
    gem "jekyll-remote-theme"
end